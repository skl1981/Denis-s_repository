Set-VMDvdDrive -VMName VM_Skakalski_2 -Path F:\Images\Windows_server_2019\17763.737.190906-2324.rs5_release_svc_refresh_SERVER_EVAL_x64FRE_en-us_1.iso
Set-VM -Name VM_Skakalski_2 -CheckpointType Disabled
Start-VM VM_Skakalski_2
